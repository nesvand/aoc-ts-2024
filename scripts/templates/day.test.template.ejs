// Advent of Code - Day <%= data.dayNumber %>
import { expect, test } from 'bun:test';
import { readFileSync } from 'node:fs';
import { part1, part2 } from '../src/<%= data.dayName %>';

let input = '';
try {
    input = readFileSync('src/<%= data.dayName %>/resources/input.txt', 'utf8');
} catch (e) {
    // ignore
}

test('part one test', () => {
    // biome-ignore lint/style/noUnusedTemplateLiteral: <explanation>
    expect(part1(``)).toBe(0);
});

if (input !== '') {
    test('part one answer', () => {
        expect(part1(input)).toBe(0);
    });
}

test('part two test', () => {
    // biome-ignore lint/style/noUnusedTemplateLiteral: <explanation>
    expect(part2(``)).toBe(0);
});

if (input !== '') {
    test('part two answer', () => {
        expect(part2(input)).toBe(0);
    });
}
